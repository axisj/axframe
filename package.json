{
  "name": "axframe",
  "version": "0.1.3",
  "scripts": {
    "preinstall": "git submodule init && git submodule update",
    "make:program": "ts-node -T src/@core/scripts/makeProgram/index.ts",
    "build:less": "ts-node -T src/@core/scripts/compile-less.ts",
    "watch": "run-p watch:*",
    "watch:less": "onchange -f change -d 1000 'src/styles/less' 'src/styles/theme.ts' -- run-s build:less",
    "dev": "npm run build:less && run-p start:alpha watch",
    "build": "tsc && vite build --mode production",
    "build:alpha": "tsc && vite build --mode alpha",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "format": "prettier --write --cache .",
    "preview": "vite preview",
    "start:alpha": "vite --mode alpha"
  },
  "dependencies": {
    "@ant-design/icons": "^5.2.6",
    "@axframe/contextmenu": "^0.3.5",
    "@axframe/datagrid": "^0.6.12",
    "@axframe/icon": "^0.1.15",
    "@ctrl/tinycolor": "^4.0.2",
    "@emotion/css": "^11.10.0",
    "@emotion/react": "^11.10.0",
    "@emotion/styled": "^11.10.0",
    "antd": "^5.9.4",
    "axios": "^1.5.1",
    "chart.js": "^4.4.0",
    "fs-extra": "^11.1.1",
    "idb-keyval": "^6.2.1",
    "less": "^4.2.0",
    "lodash": "^4.17.21",
    "lzutf8": "^0.6.3",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-css-theme-switcher": "^0.3.0",
    "react-daum-postcode": "^3.1.3",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.16.0",
    "react-sortablejs": "^6.1.4",
    "recharts": "^2.8.0",
    "uuid": "^9.0.1",
    "zustand": "^4.4.2"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.2",
    "@types/jest": "^29.5.5",
    "@types/less": "^3.0.4",
    "@types/lodash": "^4.14.199",
    "@types/node": "^20.8.2",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@types/uuid": "^9.0.4",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "npm-run-all": "^4.1.5",
    "npm-watch": "^0.11.0",
    "onchange": "^7.1.0",
    "prettier": "^3.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "vite": "^4.4.10",
    "vite-tsconfig-paths": "^4.2.1"
  }
}
